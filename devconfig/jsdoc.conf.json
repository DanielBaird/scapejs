{
    "tags": {
        "allowUnknownTags": true
    },

    "source": {
        "include": "./src",
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },

    "plugins": [],

    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },

    "opts": {
        "destination": "./docs",
        "recurse": true,
        "encoding": "utf8"
    }
}